# Example environment variables for Gold Price Prediction API
# Copy this file to ../.env and fill in your actual credentials

# Databricks MLflow credentials (if using model registry)
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-databricks-token-here

# Model paths (optional - defaults are set in docker-compose.yaml)
SCALER_PATH=/app/data/processed/scaler.pkl
FEATURE_COLS_PATH=/app/data/processed/feature_columns.json
MODEL_METADATA_PATH=/app/data/processed/model_metadata.json

